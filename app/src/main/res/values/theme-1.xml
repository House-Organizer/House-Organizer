        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/t1_light_purple</item>
        <item name="colorPrimaryVariant">#774F63</item>
        <item name="colorOnPrimary">@color/t1_beige</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/t1_dark_purple</item>
        <item name="colorSecondaryVariant">@color/t1_purple</item>
        <item name="colorOnSecondary">@color/t1_dark_purple</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>
</resources>