<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme2.HouseOrganizer" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/t2_seafoam_green</item>
        <item name="colorPrimaryVariant">@color/t2_teal</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/t2_ashy_green</item>
        <item name="colorSecondaryVariant">@color/t2_light_green</item>
        <item name="colorOnSecondary">@color/t2_ashy_green</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorSecondary</item>
        <item name="android:background" tools:targetApi="l">"#F5F5F5"</item>
    </style>
</resources>