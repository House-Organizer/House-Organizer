<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme2.HouseOrganizer" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/t2_seafoam_green</item>
        <item name="colorPrimaryVariant">@color/t2_teal</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/t2_ashy_teal</item>
        <item name="colorSecondaryVariant">@color/t2_dark_teal</item>
        <item name="colorOnSecondary">@color/t2_teal</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>
</resources>